<script type="text/javascript">
    var sysBaseUrl = "/";
    var map = null;
    //地图中心数组
    var mapCenterArr = {
        "123": [113.251237, 22.953471],
        "456": [113.23553, 22.959794],
    };
    //var stationColor1 = '#bfbfbf';
    var stationColor1 = 'rgba(6, 127, 228, 0.71)';
    var stationColor2 = '#fff';
    var stationOpacity2 = 1;
    var stationOpacity1 = 1;
    //-----
    var titleTextColor = '#fff';
    //进度正常
    var titleNormalBackground = '#2E8B57';
    //严重滞后
    var titleSeriousLagBackground = '#FF0000';
    //进度滞后
    var titleLaggingBackground = '#FF8C00';
    var titleBackgroudOpacity = 1;
    var titleBackgroudColor2 = '#49C685';
    var titleBorderColor = '#adb1af';
    var titleBorderColor2 = '#142';

    //折线
    //var polylineColor='#bfbfbf';
    var polylineColor = 'rgba(6, 127, 228, 0.71)';
    //定义不同缩放级别的点直径
    var CircleRadiusDefine = [];
    CircleRadiusDefine[3] = 20000;
    CircleRadiusDefine[4] = 12000;
    CircleRadiusDefine[5] = 8000;
    CircleRadiusDefine[6] = 5000;
    CircleRadiusDefine[7] = 3000;
    CircleRadiusDefine[8] = 1500;
    CircleRadiusDefine[9] = 1000;
    CircleRadiusDefine[10] = 500;
    CircleRadiusDefine[11] = 350;
    CircleRadiusDefine[12] = 190;
    CircleRadiusDefine[13] = 110;
    CircleRadiusDefine[14] = 65;
    CircleRadiusDefine[15] = 42;
    CircleRadiusDefine[16] = 30;
    CircleRadiusDefine[17] = 16;
    CircleRadiusDefine[18] = 10;
    CircleRadiusDefine[19] = 6;
    CircleRadiusDefine[20] = 4;
    //定义不同缩放级别的线宽
    var LineWeightDefine = [];
    LineWeightDefine[3] = 1;
    LineWeightDefine[4] = 1;
    LineWeightDefine[5] = 1;
    LineWeightDefine[6] = 1;
    LineWeightDefine[7] = 1;
    LineWeightDefine[8] = 1;
    LineWeightDefine[9] = 1;
    LineWeightDefine[10] = 1;
    LineWeightDefine[11] = 2;
    LineWeightDefine[12] = 3;
    LineWeightDefine[13] = 4;
    LineWeightDefine[14] = 5;
    LineWeightDefine[15] = 6;
    LineWeightDefine[16] = 7;
    LineWeightDefine[17] = 8;
    LineWeightDefine[18] = 9;
    LineWeightDefine[19] = 10;
    LineWeightDefine[20] = 11;
    $(function () {
        initBranchId();
        //初始化二维地图
        switch2DMap();
    });
    //当前分部组织机构
    var currentBranchId;
    //轨道
    var trackBranchId;
    //供电
    var powerSupplyBranchId;
    //机电
    var electromechanicalBranchId;
    var switchMapText = 'switch2DMap';
    //获取默认分部机构id
    function initBranchId() {
        var orgDataArray = [];
        $.ajax({
            url: "@Url.Action("GetDicByCode", "DataDictionary", new { area = "SystemManage", dicCode = "SystemSeting" })",
            dataType: "json",
            async: false,
            success: function (data) {
                if (data != null) {
                    orgDataArray = data;
                }
            }
        });
        if (orgDataArray.length > 0) {
            for (var i = 0, n = orgDataArray.length; i < n; i++) {
                var obj = orgDataArray[i];
                if (obj.DictionaryCode == '轨道') {
                    trackBranchId = obj.DictionaryText
                }
                if (obj.DictionaryCode == '供电') {
                    powerSupplyBranchId = obj.DictionaryText
                }
                if (obj.DictionaryCode == '机电') {
                    electromechanicalBranchId = obj.DictionaryText
                }
            }
            if (electromechanicalBranchId != undefined) {
                currentBranchId = electromechanicalBranchId;
            }
        }
    }
    //轨道
    function switchTrack() {
        currentBranchId = trackBranchId;
        switchCurrentMap();
    }
    //供电
    function switchPowerSupply() {
        currentBranchId = powerSupplyBranchId;
        switchCurrentMap();
    }
    //机电
    function switchElectromechanical() {
        currentBranchId = electromechanicalBranchId;
        switchCurrentMap();
    }
    function switchCurrentMap() {
        if (switchMapText == 'switch2DMap') {
            switch2DMap();
        }
        if (switchMapText == 'switch3DMap') {
            switch3DMap();
        }
        if (switchMapText == 'switchSatelliteMap') {
            switchSatelliteMap();
        }
    }
    //切换二维地图
    function switch2DMap() {
        //存在地图则销毁地图
        destroyMap();
        //获取地图中心位置
        var mapCenter = mapCenterArr['456'];
        //地图样式
        var mapStyle = 'amap://styles/4232ebc28581310f2d632a7b5973446d';
        //创建地图
        map = new AMap.Map('container', {
            viewMode: '2D', // 地图模式
            mapStyle: mapStyle,
            center: mapCenter,
            zoom: 13,
            //地图显示的缩放级别范围, 默认为 [2, 20] ，取值范围 [2 ~ 20]
            zooms: [12, 20],
        });
        //创建右键菜单
        setupRightClickMenu(map);
        var dataMapArray = getData();
        var len = dataMapArray.length
        for (var i = 0; i < len; i++) {
            var dataMap = dataMapArray[i];
            //初始化站点图形
            var circleVisible = dataMap.Circle == null ? false : dataMap.Circle;
            if (circleVisible) {
                var circle = setCircle(dataMap.CentrePoint);
                map.add(circle);
            }
            //初始化站点标题
            var marker = setMarker(dataMap);
            //设置查看模型窗口
            setViewModelWindow(map, marker, dataMap.CompanyCode);
            map.add(marker);
        }
        //折线
        var lineData = getLineData();
        var lineLen = lineData.length;
        for (var i = 0; i < lineLen; i++) {
            var lineArray = lineData[i].LineArray;
            if (lineArray != null && lineArray != "") {
                var polyline = setPolyline(lineArray);
                map.add(polyline);
            }
        }
        switchMapText = 'switch2DMap';
    }

    //切换三维地图
    function switch3DMap() {
        //存在地图则销毁地图
        destroyMap();
        //获取地图中心位置
        var mapCenter = mapCenterArr['456'];
        //创建地图
        map = new AMap.Map('container', {
            pitch: 65, // 地图俯仰角度，有效范围 0 度- 83 度
            viewMode: '3D', // 地图模式
            center: mapCenter,
            zoom: 13,
            //地图显示的缩放级别范围, 默认为 [2, 20] ，取值范围 [2 ~ 20]
            zooms: [12, 20],
        });
        //创建右键菜单
        setupRightClickMenu(map);
        var dataMapArray = getData();
        var len = dataMapArray.length
        for (var i = 0; i < len; i++) {
            var dataMap = dataMapArray[i];
            //初始化站点图形
            var circleVisible = dataMap.Circle == null ? false : dataMap.Circle;
            if (circleVisible) {
                var circle = setCircle(dataMap.CentrePoint);
                map.add(circle);
            }
            //初始化站点标题
            var marker = setMarker(dataMap);
            //设置查看模型窗口
            setViewModelWindow(map, marker, dataMap.CompanyCode);
            map.add(marker);
        }
        var lineData = getLineData();
        var lineLen = lineData.length;
        for (var i = 0; i < lineLen; i++) {
            var lineArray = lineData[i].LineArray;
            if (lineArray != null && lineArray != "") {
                var polyline = setPolyline(lineArray);
                map.add(polyline);
            }
        }
        switchMapText = 'switch3DMap';
    }

    //切换卫星地图（实景）
    function switchSatelliteMap() {
        //存在地图则销毁地图
        destroyMap();
        //获取地图中心位置
        var mapCenter = mapCenterArr['456'];
        //创建地图
        map = new AMap.Map('container', {
            center: mapCenter,
            //卫星地图
            layers: [new AMap.TileLayer.Satellite()],
            zoom: 13,
            //地图显示的缩放级别范围, 默认为 [2, 20] ，取值范围 [2 ~ 20]
            zooms: [12, 20],
        });
        //创建右键菜单
        setupRightClickMenu(map);
        var dataMapArray = getData();
        var len = dataMapArray.length
        for (var i = 0; i < len; i++) {
            var dataMap = dataMapArray[i];
            //初始化站点图形
            var circleVisible = dataMap.Circle == null ? false : dataMap.Circle;
            if (circleVisible) {
                var circle = setCircle(dataMap.CentrePoint);
                map.add(circle);
            }
            //初始化站点标题
            var marker = setMarker(dataMap);
            //设置查看模型窗口
            setViewModelWindow(map, marker, dataMap.CompanyCode);
            map.add(marker);
        }
        var lineData = getLineData();
        var lineLen = lineData.length
        for (var i = 0; i < lineLen; i++) {
            var lineArray = lineData[i].LineArray;
            if (lineArray != null && lineArray != "") {
                var polyline = setPolyline(lineArray);
                map.add(polyline);
            }
        }
        switchMapText = 'switchSatelliteMap';
    }

    function getData() {
        var dataMapArray = [];
        if (currentBranchId != undefined) {
            $.ajax({
                url: sysBaseUrl + "SystemManage/OrganizationMap/GetOrganizationMapList?Type=1&SiteCode=" + currentBranchId,
                dataType: "json",
                async: false,//同步
                success: function (resultdata) {
                    if (resultdata.data != null) {
                        dataMapArray = resultdata.data;
                    }
                },
                error: function (data, errorMsg) {
                    console.log(errorMsg)
                }
            });
        }
        return dataMapArray;
    }

    function getLineData() {
        var lineData = [];
        if (currentBranchId != undefined) {
            $.ajax({
                type: "GET",
                url: sysBaseUrl + "SystemManage/OrganizationMap/GetOrganizationMapList?Type=2&SiteCode=" + currentBranchId,
                dataType: "json",
                async: false,//同步
                success: function (resultdata) {
                    if (resultdata.data != null) {
                        lineData = resultdata.data;
                    }
                },
                error: function (data, errorMsg) {
                    console.log(errorMsg)
                }
            });
        }
        return lineData;
    }
    //初始化站点标题
    function setMarker(dataMap) {
        //判断滞后
        var titleBackground = titleNormalBackground;
        //进度滞后
        if (dataMap.LineArray == '2') {
            titleBackground = titleLaggingBackground;
        }
        //严重滞后
        if (dataMap.LineArray == '3') {
            titleBackground = titleSeriousLagBackground;
        }
        var xOffset = 0;
        var yOffset = 0;
        if (dataMap.Deviation != null && dataMap.Deviation != "") {
            var bool = $.isArray(eval(dataMap.Deviation));
            if (bool) {
                xOffset = eval(dataMap.Deviation)[0];
                yOffset = eval(dataMap.Deviation)[1]
            }
        }
        var markerVisible = dataMap.Marker == null ? false : dataMap.Marker;
        var marker = new AMap.Marker({
            topWhenClick: true,
            //点标记是否可见
            visible: markerVisible,
            position: eval(dataMap.CentrePoint), //基点位置
            //相对于基点的偏移位置
            offset: new AMap.Pixel(xOffset, yOffset),
            content: '<div name="left-right-click" org_id="' + dataMap.CompanyCode_F + '" centre_point="' + dataMap.CentrePoint + '"  onclick="openSiteInfoWindow(\'' + dataMap.CentrePoint + '\',\'' + dataMap.Name + '\')" style="background:' + titleBackground + ';opacity:' + titleBackgroudOpacity + ';border:1px solid ' + titleBorderColor + ';box-shadow: 2px 2px 2px #888888;padding:2px 3px;border-radius:4px;color:' + titleTextColor + ';white-space:nowrap;text-align: center;width: 114px"><p style="font-size:12px;">' + dataMap.Name + '</p>' +
                '<p style="display: -webkit-inline-box;-webkit-transform:scale(0.8);">' + dataMap.CompanyCode_F + '</p>' + '</div>',   //自定义点标记覆盖物内容
            extData: '1234',
            zIndex: 15,
        });
        return marker;
    }
    //打开站点明细窗口
    function openSiteInfoWindow(centrePoint, name) {
        var infoWindow = new AMap.InfoWindow({
            //自定义窗口
            isCustom: true,
            anchor: 'top-left',
            content: '<div style="width: 120px;height: 100px;border-radius: 5px;background-color: rgba(175,238,238, .9);">' +
                '<div style="width: 120px;text-align: center;font-size: 12px;font-weight:700;padding-top:5px">' + name + '</div>' +
                '<div class="hr hr-dotted" style="margin: 2px 0;"></div>' +
                '<div style="width: 120px;text-align: center;font-size: 12px;display: -webkit-inline-box;-webkit-transform:scale(0.9);"><类型数/名称数/件数></div>' +
                '<div class="site-info-window-item" >' +
                '<div class="site-info-window-left">计划总数：</div>' +
                '<div class="site-info-window-right">10/20/200</div></div>' +
                '<div class="site-info-window-item" style="color: #2E8B57">' +
                '<div class="site-info-window-left">已完成：</div>' +
                '<div class="site-info-window-right">2/5/60</div></div>' +
                '<div class="site-info-window-item" style="color: #808080">' +
                '<div class="site-info-window-left">未完成：</div>' +
                '<div class="site-info-window-right">8/15/140</div></div>' +
                '<div class="site-info-window-item" style="color: #FF0000">' +
                '<div class="site-info-window-left">滞后未完成：</div>' +
                '<div class="site-info-window-right" title="6/8/450000000000">6/8/450000000000</div></div>' +
                '</div>',
            //控制是否在鼠标点击地图后关闭信息窗体
            closeWhenClickMap: true,
            //偏移
            offset: new AMap.Pixel(50, 20)
        });
        infoWindow.open(map, eval(centrePoint));
    }
    //设置查看模型窗口
    function setViewModelWindow(map, marker, orgId) {
        // 菜单内容
        var content = [
            '<div onclick="view3DModel(\'' + orgId + '\')" style="width: 80px;height: 18px;cursor:pointer;padding:3px 3px;text-align:center;box-shadow: 0 0 4px rgba(0, 0, 0, .7);font-size:12px;background-color: #f0f0f0">' +
            '查看模型</div>'
        ];
        //创建右键菜单
        var contextMenu = new AMap.ContextMenu({
            isCustom: true,
            content: content.join('')
        });

        //绑定鼠标右击事件——弹出右键菜单
        marker.on('rightclick', function (e) {
            contextMenu.open(map, e.lnglat);
        });
    }
    //查看模型
    function view3DModel(siteCode) {
        show3DModel_Index(siteCode);
    }
    //初始化站点图形
    function setCircle(centrePoint) {
        var circle = new AMap.Circle({
            center: eval(centrePoint),// 圆心位置
            radius: CircleRadiusDefine[map.getZoom()] * 0.8, //半径
            strokeColor: stationColor2, //线颜色
            strokeOpacity: stationOpacity2, //线透明度
            strokeWeight: 2, //线粗细度
            fillColor: stationColor1, //填充颜色
            fillOpacity: stationOpacity1,//填充透明度
            zIndex: 16
        });
        return circle;
    }
    //设置线条路径和样式
    function setPolyline(lineArray) {
        var polyline = new AMap.Polyline({
            path: eval(lineArray),//设置线覆盖物路径
            isOutline: true, //线条是否带描边，默认false
            outlineColor: '#ffeeff',//线条描边颜色，此项仅在isOutline为true时有效，默认：#000000
            borderWeight: 3, //描边的宽度，默认为1
            strokeColor: polylineColor, //线条颜色，使用16进制颜色代码赋值。默认值为#006600
            strokeOpacity: 1, //线条透明度，取值范围[0,1]，0表示完全透明，1表示不透明。默认为0.9
            strokeWeight: LineWeightDefine[map.getZoom()],        //线条宽度，单位：像素
            // 折线样式还支持 'dashed'
            strokeStyle: "solid", //线样式，实线:solid，虚线:dashed
            // strokeStyle是dashed时有效
            strokeDasharray: [10, 5],//勾勒形状轮廓的虚线和间隙的样式，此属性在strokeStyle 为dashed 时有效
            lineJoin: 'round', //折线拐点的绘制样式，默认值为'miter'尖角，其他可选值：'round'圆角、'bevel'斜角
            lineCap: 'round', //折线两端线帽的绘制样式，默认值为'butt'无头，其他可选值：'round'圆头、'square'方头
            zIndex: 10, //折线覆盖物的叠加顺序。默认叠加顺序，先添加的线在底层，后添加的线在上层。通过该属性可调整叠加顺序，使级别较高的折线覆盖物在上层显示默认zIndex：50
        });
        return polyline;
    }

    //设置右键菜单
    function setupRightClickMenu(map) {
        //创建右键菜单
        var contextMenu = new AMap.ContextMenu();
        //右键放大
        contextMenu.addItem("放大一级", function () {
            map.zoomIn();
        }, 0);
        //右键缩小
        contextMenu.addItem("缩小一级", function () {
            map.zoomOut();
        }, 1);
        //右键添加Marker标记
        contextMenu.addItem("添加标记", function (e) {
            var marker = new AMap.Marker({
                map: map,
                position: contextMenuPositon //基点位置
            });
        }, 3);
        //地图绑定鼠标右击事件——弹出右键菜单
        map.on('rightclick', function (e) {
            contextMenu.open(map, e.lnglat);
            contextMenuPositon = e.lnglat;
        });
    }

    //地图销毁
    function destroyMap() {
        map && map.destroy();
    }

    $(function () {
        //地图选择 点击变颜色
        $("a[name='switch-map-click']").click(function () {
            $("a[name='switch-map-click']").eq($(this).index()).addClass("navdown").siblings().removeClass("navdown");
        })
        //专业选择 点击变颜色
        $("div[name='work-type-click']").click(function () {
            $("div[name='work-type-click']").eq($(this).index()).addClass("boder-down").siblings().removeClass("boder-down");
        })
    })
</script>
