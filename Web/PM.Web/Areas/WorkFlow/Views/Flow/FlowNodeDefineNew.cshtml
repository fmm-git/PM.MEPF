            function BackData() {
                var tableOrderData = GetGridRowData();
                if (tableOrderData.length < 1) {
                    $.modalMsg("请选择审批、抄送人员", "warning");
                    return false;
                }
                var Result = {};
                Result["SPLIST"] = tableOrderData;
                var Name = $("#FlowNodeName").val();
                Result["Name"] = Name;
                var AllApproval = $("#AllApproval").is(":checked") ? 1 : 0;
                Result["AllApproval"] = AllApproval;
                var FreeCandidates = $("#FreeCandidates").is(":checked") ? 1 : 0;
                Result["FreeCandidates"] = FreeCandidates;
                var BlankNode = $("#BlankNode").is(":checked") ? 1 : 0;
                Result["BlankNode"] = BlankNode;
                var FlowNodeEvent = $("#FlowNodeEvent").val();
                Result["FlowNodeEvent"] = FlowNodeEvent;
                return Result;
            }
            //获取表格数据
            function GetGridRowData() {
                var columnData = [];
                $("#gridList1").jqGrid('saveRow', lastsel1);
                var ids1 = $("#gridList1").jqGrid('getDataIDs');//获取多行的id
                $(ids1).each(function (index, yu) {
                    var rowData = $("#gridList1").jqGrid("getRowData", yu);
                    columnData.push(rowData);
                });
                $("#gridList2").jqGrid('saveRow', lastsel2);
                var ids2 = $("#gridList2").jqGrid('getDataIDs');//获取多行的id
                $(ids2).each(function (index, yu) {
                    var rowData = $("#gridList2").jqGrid("getRowData", yu);
                    columnData.push(rowData);
                });
                $("#gridList3").jqGrid('saveRow', lastsel3);
                var ids3 = $("#gridList3").jqGrid('getDataIDs');//获取多行的id
                $(ids3).each(function (index, yu) {
                    var rowData = $("#gridList3").jqGrid("getRowData", yu);
                    columnData.push(rowData);
                });
                $("#gridList4").jqGrid('saveRow', lastsel4);
                var ids4 = $("#gridList4").jqGrid('getDataIDs');//获取多行的id
                $(ids4).each(function (index, yu) {
                    var rowData = $("#gridList4").jqGrid("getRowData", yu);
                    columnData.push(rowData);
                });
                return columnData;
            }
            //显示流程事件
            function ShowFlowEvent() {
                var FlowNodeEvent= $("#FlowNodeEvent").val();
                layer.open({
                    type: 2,
                    title: "流程事件",
                    area: ['95%', '95%'],
                    content: "/WorkFlow/Flow/FlowNodeEvent?FormCode=" + FormCode + "&FlowNodeEvent="+FlowNodeEvent,
                    btn: ['确定', '关闭'],
                    yes: function (index, layero) {
                        var Result = layero.find("iframe")[0].contentWindow.BackData();
                        if (Result) {
                            $("#FlowNodeEvent").val(Result);
                        }
                        layer.close(index);
                    }, cancel: function (index) { }
                });
            }
