            dataType: "json",
            async: false,
            success: function (data) {
                if (data.length > 0) {
                    for (var i = 0; i < data.length; i++) {
                        if (data[i]["GhType"] != "供货") {
                            Data1.push({
                                name: data[i]["GhType"],
                                y: data[i]["GhWeight"],
                                cs: data[i]["GhCount"]
                            });
                        } else {
                            ghcs = data[i]["GhCount"];
                            ghl = data[i]["GhWeight"];
                        }
                    }
                }
            }
        });
