            plotOptions: {
                series: {
                    borderWidth: 0,
                    dataLabels: {
                        enabled: true,
                        format: '<span style="color:#757575">{point.y}</span>',
                    }
                }
            },
            tooltip: {
                pointFormat: '<span style="color:#757575">{point.y}</span>'
